{
	"name": "obsidian-snippets",
	"version": "1.0.0",
	"description": "A snippet plugin for obsidian",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "jest"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@types/marked": "^4.0.8",
		"front-matter": "^4.0.2",
		"marked": "^4.2.5"
	},
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"jest": "^26.6.3",
		"@types/jest": "^29.2.5",
		"jest-cli": "^26.6.3",
		"ts-jest": "^26.4.3",
		"ts-node": "^10.9.1"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "./",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": [
				"ts-jest",
				{
					"useESM": true
				}
			]
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node",
		"moduleNameMapper": {
			"^(\\.{1,2}/.*)\\.js$": "$1"
		}
	}
}